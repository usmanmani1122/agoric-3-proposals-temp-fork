{
  "name": "@agoric/synthetic-chain",
  "version": "0.0.9",
  "description": "Utilities to build a chain and test proposals atop it",
  "bin": "dist/cli/cli.js",
  "main": "./dist/lib/index.js",
  "type": "module",
  "module": "./dist/lib/index.js",
  "exports": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "yarn build",
    "build": "tsup",
    "test": "NODE_OPTIONS='--loader=tsx --no-warnings' ava"
  },
  "engines": {
    "node": "^18.19 || ^20.9"
  },
  "dependencies": {
    "@endo/zip": "^1.0.1",
    "better-sqlite3": "^9.4.0",
    "chalk": "^5.3.0",
    "execa": "^8.0.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.9",
    "@types/node": "^18.19.14",
    "ava": "^5.3.1",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "ava": {
    "extensions": {
      "js": true,
      "ts": "module"
    }
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  }
}
